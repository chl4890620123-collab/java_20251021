package test;

public class sample {

	public static void main(String[] args) {
		
		System.out.println("Hello, Java");
		System.out.println("Hello, Java");
	char var1 ='A';
	char c1= 65;
	
	char var2 ='가';
	//char c2 = 44032;
	char c2= 0xD7A3;
	
	System.out.println(var1);
	System.out.println(c1);
	
	System.out.println(var2);
	System.out.println(c2);
	
	float f1 = 10.5f;
	
	double d1 = 10.5;
	
	System.out.println(f1);
	System.out.println(d1);
	
	System.out.println(var1);
	System.out.println(c1);
	
	float f2 = 2147483648L;
	float f3 = 2147483558L;
	float f4 = 2147483668L;
	
	System.out.println(f2);
	System.out.println(f3);
	System.out.println(f4);
	
	//2,1474836E9 2.1474836x 10 -> 2,147,483,600

	String str1 = "최\t명\t헌"; // \t tap만큼 듸움
	String str2 = "홍\n길\n동"; // \n 줄 변경

	// 자동 형 변환
	System.out.println(str1);
	System.out.println(str2);

	int intA= 10000;
	byte byteB= 20;
	
	//intA = byteB; //a = (int)b;
	
	System.out.println(intA);
	System.out.println(byteB);
	
	//강제 형 변환
	
	System.out.println("---강제 형 변환----");
	byteB = (byte)intA;
	
	System.out.println(intA);
	System.out.println(byteB);
	
	
	
	}

}
